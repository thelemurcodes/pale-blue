<div class="card" id="home">
  <%= link_to creator_path(creator) do %>
    <div class="box frame">
      <% if creator.photo.key.present?  %>
        <img style="border: 1px white solid" src= <%= cl_image_path creator.photo.key %>>
      <% else %>
        <%= image_tag "banner", style: "border: 1px white solid" %>
      <% end %>
    </div>
  <% end %>
  <h4><% if creator.name.present? %>
        <%= creator.name %>
        <% else  %>
        <%= Faker::Twitter.screen_name %>
      <% end %></h4>
  <p>Total Volume: $<%= rand(5000...25000) %></p>
</div>
